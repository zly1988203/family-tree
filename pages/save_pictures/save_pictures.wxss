/* pages/save_pictures/save_pictures.wxss */
.container {
  background: #fff;
  padding-top: 25px;
}
view{font-size: 32rpx;}
.page-content {
  width: 100%;
  height: calc(100% - 34px);
}
.page-content .main{
  background: #f7f7f7;
}
.canvas-cover{
  position: fixed;
  top: calc(9999vmin / 7.5);
  left: 0;
}
.tar-img{
  margin: calc(40vmin / 7.5) calc(30vmin / 7.5) 0;
  background: #fff;
  border-radius: calc(15vmin / 7.5);
  width: calc(690vmin / 7.5);
  height: calc(880vmin / 7.5);
}
.tar-img image{
  width: 100%;
  height: 100%;
}
.tar-img .help-block{
  width: 100%;
  height: 100%;
}
.tar-img .help-block::before{
  content: " ";
  width: 0;
  height: 100%;
  display: inline-block;
  vertical-align: middle;
}
.tar-img .help-block .help-info{
  width: 100%;
  display: inline-block;
  vertical-align: middle;
  text-align: center;
}
.tar-img .help-block .help-info .info-text{
  font-size: calc(32vmin / 7.5);
  text-align: center;
  line-height: calc(60vmin / 7.5);
  margin-bottom: calc(60vmin / 7.5);
}
.tar-img .help-block .help-info .info-text text{
  font-size: calc(32vmin / 7.5);
  text-align: center;
  line-height: calc(58vmin / 7.5);
  display: block;
  width: 100%;
  font-weight: bold;
}
.tar-img .help-block .help-info .info-text text.info{
  font-size: calc(26vmin / 7.5);
  color: #696969;
  font-weight: normal;
}
.tar-img .help-block .help-info .get-userinfo{
  background: #f2f2f2;
  font-weight: normal;
  border: none;
  border-radius: calc(5vmin / 7.5);
  text-align: center;
  font-size: calc(32vmin / 7.5);
  color: #ebb450;
}
.tqm{display: flex;flex-direction: row;margin-top: calc(72vmin / 7.5);}
.tqm view:first-child{margin-left: 200rpx;line-height: 48rpx;}
.tqm view:nth-child(2){margin: 0 24rpx 0 50rpx;width: 108rpx;height: 48rpx;text-align: center;line-height: 48rpx;
  font-size: 30rpx;margin-right: 26rpx;}
.tqm view:last-child{color: #ebb450;line-height: 48rpx;}
.bc_img{margin: 34rpx 40rpx 0 30rpx;text-align: center;line-height: 90rpx;height: 90rpx;color: #fff;font-weight: bold;
  background: #ebb450;border-radius: 45rpx;}