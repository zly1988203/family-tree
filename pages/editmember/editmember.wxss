.b_list .b_list_left,.b_list .b_list_right{width: 50%;display: flex;}
.b_list input{height: 96rpx;line-height: 96rpx;font-size: 26rpx;}
.b_list .gender{display: flex;justify-content: space-between;}
.special_relationship{margin:0 30rpx;display:flex;}
.special_relationship .title{width: 270rpx;font-size: 30rpx;padding-top: 20rpx;}
