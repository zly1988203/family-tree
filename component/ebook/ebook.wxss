.jiapu-searchlist-content .search-list-dialog .close{
  position: absolute;
  right:calc(-30vmin / 7.5);
  top:calc(-30vmin / 7.5);
  height: calc(60vmin / 7.5);
  width: calc(60vmin / 7.5);
  display: block;
