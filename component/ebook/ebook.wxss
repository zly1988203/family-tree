/* component/ebook/ebook.wxss */
.jiapu-outside-box{
  width: 100%;
  height: 100%;
  overflow: hidden;
  padding: 0;
}
.jiapu-search-form{
  height: calc(107vmin / 7.5);
  padding: calc(24vmin / 7.5) calc(30vmin / 7.5);
}
.jiapu-search-form .form-group,.jiapu-search-form .input-group{
  width: 100%;
  height: 100%;
}
.jiapu-search-form .form-group.inside .input-group .form-control{
  width: 100%;
  height: 100%;
  padding: 0 calc(100vmin / 7.5) 0 calc(75vmin / 7.5);
  background: #fff;
  border-radius: calc(30vmin / 7.5);
  line-height: calc(60vmin / 7.5);
  box-sizing: border-box;
}
.jiapu-search-form .form-group.inside .input-group .icon{
  position: absolute;
  width: calc(26vmin / 7.5);
  height: calc(26vmin / 7.5);
  left: calc(45vmin / 7.5);
  top:50%;
  transform: translate(-50%,-50%);
  background-size: 100% 100%;
  background-position: center center;
  background-repeat: no-repeat;
}
.jiapu-search-form .form-group.inside .btn-search{
  padding: 0!important;
  margin: 0!important;
  width: calc(70vmin / 7.5)!important;
  min-height: 0!important;
  position: absolute;
  right: calc(30vmin / 7.5);
  left: auto;
  font-weight: normal;
  top:50%;
  background: transparent;
  transform: translateY(-50%);
  color: #ebb450;
  font-size: calc(30vmin / 7.5);
  line-height: calc(60vmin / 7.5 - 2px);
}
.jiapu-content{
  width: 100%;
  height: calc(100% - 170vmin / 7.5);
  background: #fff;
  overflow: hidden;
  white-space: nowrap;
}
.jiapu-content::before,.jiapu-content::after{
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  right: auto;
  width: 0;
  box-shadow: 0 0 calc(35vmin / 7.5) calc(10vmin / 7.5) #d9d9db;
}
.jiapu-content::after{
  left: auto;
  right: 0;
}
@keyframes left_in{
  0%{
    width: 0;
    display: inline-block;
  }
  100%{
    width: calc(74vmin / 7.5);
    display: inline-block;
  }
}
@keyframes left_out{
  0%{
    width: calc(74vmin / 7.5);
    display: inline-block;
  }
  99%{
    width: 0;
    display: inline-block;
  }
  100%{
    width: 0;
    display: none;
  }
}
.jiapu-content .jiapu-next-end,.jiapu-content .jiapu-prev-end{
  display: inline-block;
  line-height: calc(74vmin / 7.5);
  writing-mode:tb-rl;
  font-size: calc(26vmin / 7.5);
  color: #696969;
  height: 100%;
  text-align: center;
  vertical-align: top;
}
.jiapu-content .jiapu-next-end{
	animation: left_out 1s ease 0s 1 normal forwards;
}
.jiapu-content .jiapu-prev-end.active{
  width: calc(74vmin / 7.5);
  display: inline-block;
}
.jiapu-content .jiapu-next-end.active{
  width: calc(74vmin / 7.5);
  display: inline-block;
	animation: left_in 1s ease 0s 1 normal forwards;
}
.jiapu-content .jiapu-show{
  display: inline-block;
  vertical-align: top;
  height: 100%;
  padding: calc(19vmin / 7.5) 0;
  width: 100%;
  transition: all 1s;
}
.jiapu-content .jiapu-show .jiapu-box{
  width: 100%;
  height: 100%;
  border: 4px solid #000;
  border-radius: 3px;
  padding: 4px 5px;
}
.jiapu-content .jiapu-base{
  border: 2px solid #3d3d3d;
  border-right: none;
  width: calc(100% - 2px - 94vmin / 7.5);
  height: 100%;
  display: inline-block;
  vertical-align: top;
  box-sizing: border-box;
  overflow: hidden;
}
.jiapu-content .jiapu-base .member-list{
  height: 100%;
  text-align: right;
  white-space: nowrap;
  width: auto;
}
.jiapu-content .jiapu-base .member-item{
  display: inline-block;
  vertical-align: top;
  /*min-width: calc(60vmin / 7.5);*/
  width: auto;
  height: 100%;
  border-left: 1px solid #000;
  font-size: 0;
}
.jiapu-content .jiapu-base .member-item:first-child{
  border-left: none;
}
.jiapu-content .jiapu-base .member-item .shi,.jiapu-content .jiapu-base .member-item .zi,.jiapu-content .jiapu-base .member-item .other,.jiapu-content .jiapu-base .member-item .name,.jiapu-content .jiapu-base .member-item .desc{
  width: 100%;
  writing-mode:tb-rl;
  border-top: 1px solid #000;
  overflow: hidden;
}
.jiapu-content .jiapu-base .member-item .shi::before,.jiapu-content .jiapu-base .member-item .zi::before,.jiapu-content .jiapu-base .member-item .other::before,.jiapu-content .jiapu-base .member-item .name::before,.jiapu-content .jiapu-base .member-item .desc::before{
  content: " ";
  display: inline-block;
  width: 100%;
  height: 0;
  vertical-align: middle;
}
.jiapu-content .jiapu-base .member-item .shi .text,.jiapu-content .jiapu-base .member-item .zi .text,.jiapu-content .jiapu-base .member-item .other .text,.jiapu-content .jiapu-base .member-item .name .text,.jiapu-content .jiapu-base .member-item .desc .text{
  height: 100%;
  writing-mode:tb-rl;
  text-align: center;
  display: inline-block;
  vertical-align: middle;
  white-space: normal;
  /*text-orientation: upright;*/
  font-size: calc(24vmin / 7.5);
}
.jiapu-content .jiapu-base .member-item .shi{
  border-top: none;
  padding: calc(20vmin / 7.5) 0;
}
.jiapu-content .jiapu-base .member-item .zi{
  padding: calc(10vmin / 7.5) 0;
}
.jiapu-content .jiapu-base .member-item .other{
  padding: calc(10vmin / 7.5) 0;
}
.jiapu-content .jiapu-base .member-item .name{
  padding: calc(5vmin / 7.5) 0;
}
.jiapu-content .jiapu-base .member-item .desc{
  padding: calc(10vmin / 7.5) 0;
  border-bottom: none;
}
.jiapu-content .jiapu-base .member-item .shi .text{
}
.jiapu-content .jiapu-base .member-item .desc .text{
  text-align: left;
}
