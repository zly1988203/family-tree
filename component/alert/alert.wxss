/* component/alert/alert.wxss */
.sweet-tips,.sweet-list,.sweet-loading{
  z-index: 999;
}
.sweet-tips{
  position: absolute;
  width: 100%;
  font-size: 26rpx;
  height: 56rpx;
  line-height: 56rpx;
  text-align: center;
  top: 0;
}
.sweet-tips text{
  width: 100%;
  text-align: center;
}
.sweet-tips .close{
  background-color: transparent;
  background-size: 100% 100%;
  background-position: center center;
  background-repeat: no-repeat;
  border: none;
  padding: 0;
  margin: 0;
  position: absolute;
  height: 26rpx !important;
  width: 26rpx !important;
  top: 50%;
  right: 28rpx;
  transform: translateY(-50%);
  min-height:unset !important;
}
.sweet-list,.sweet-list .cover{
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
}
.sweet-list .cover{
  background: rgba(0,0,0,0.5);
}
.sweet-list .cover{
  opacity: 0.5;
}
.sweet-list .sweet-list-content{
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  width: 560rpx;
  padding: 34rpx;
  border-radius: 24rpx;
  background: #fff;
}
.sweet-list .radio{
  line-height: 90rpx;
  width: 100%;
  border-bottom: 1px solid #ededed;
}
.sweet-list .radio text{
  font-size: 30rpx;
  line-height: 90rpx;
  color: #333;
}
.sweet-list .radio.active text{
  color: #ebb450;
}
.btn{
  font-weight: normal;
  margin: 12rpx 0;
  background: transparent;
  border-radius: 45rpx;
  height: 90rpx;
  line-height: 88rpx;
  font-size: 34rpx;
  border: 1px solid transparent;
  padding: 0;
  width: 100% !important;
}
.btn-submit{
  background: #ebb450;
  border-color: #ebb450;
  color: #fff;
  box-shadow: 0rpx 1rpx 5rpx 0rpx #ebb450;
}
.help-block{
  font-size: 24rpx;
  color:#696969;
  line-height: 90rpx;
  display: block;
  text-align: center;
}
.sweet-loading,.sweet-loading .cover{
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
}
.sweet-loading .cover{
  background: rgba(0,0,0,0.5);
}
.sweet-loading .sweet-loading-content{
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  width: calc(300vmin / 7.5);
}
.sweet-loading .sweet-loading-content .loading-logo{
  width: calc(100vmin / 7.5);
  height: calc(100vmin / 7.5);
  margin: 0 auto;
  background-position: center center;
  background-size: 100% auto;
  background-repeat: no-repeat;
  background-image: url("data:image/svg+xml;base64,");
}
.sweet-loading .sweet-loading-content .loading-logo.icon1{
  background-image: url("data:image/svg+xml;base64,");
}
.sweet-loading .sweet-loading-content .loading-logo.icon2{
  background-image: url("data:image/svg+xml;base64,");
}
.sweet-loading .sweet-loading-content .loading-logo.icon3{
  background-image: url("data:image/svg+xml;base64,");
}
.sweet-loading .sweet-loading-content .loading-logo.icon4{
  background-image: url("data:image/svg+xml;base64,");
}
.sweet-loading .sweet-loading-content .loading-logo.icon5{
  background-image: url("data:image/svg+xml;base64,");
}
.sweet-loading .sweet-loading-content text{
  width: 100%;
  text-align: center;
  font-size: calc(28vmin / 7.5);
  line-height: calc(60vmin / 7.5);
  color: #fff;
  display: block;
}
.sweet-payment,.sweet-payment .cover{
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
}
.sweet-payment .cover{
  background: rgba(0,0,0,0.5);
}
.sweet-payment .payment-content{
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  width: calc(600vmin / 7.5);
  padding: calc(30vmin / 7.5);
  border-radius: calc(20vmin / 7.5);
  text-align: center;
  background: #fff;
}
.sweet-payment .payment-content image{
  width: calc(540vmin / 7.5);
  height: calc(540vmin / 7.5);
  display: block;
}
.sweet-payment .payment-content .payment-info{
  display: block;
  text-align: center;
  font-size: calc(32vmin / 7.5);
  line-height: calc(90vmin / 7.5);
}
.sweet-payment .payment-content .payment-info .tips{
  color: #f00;
}
.sweet-payment .payment-content .close{
  position: absolute;
  right: calc(-30vmin / 7.5);
  top: calc(-30vmin / 7.5);
  width: calc(60vmin / 7.5)!important;
  height: calc(60vmin / 7.5)!important;
  min-height: 0!important;
  background-repeat: no-repeat;
  background-position: center center;
  background-size: 100% 100%;
  margin: 0!important;
  padding: 0!important;
}
.sweet-image{
  z-index: 2;
  overflow: hidden;
}
.sweet-image,.sweet-image .cover,.sweet-image .image-content,.sweet-image .image-cover{
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  z-index: 2;
}
.sweet-image .cover,.sweet-image .image-cover{
  background: rgba(0,0,0,0.5);
}
.sweet-image .image-show,.sweet-image .image-select,.image-select .image-c,.image-select .image-t-l,.image-select .image-t-r,.image-select .image-b-l,.image-select .image-b-r{
  position: absolute;
}
.sweet-image .image-content{
  height: 100%;
  z-index: 2;
}
.sweet-image .image-content .btn-submit{
  display: inline-block;
  vertical-align: middle;
  width: calc(50% - 60vmin / 7.5)!important;
  margin-left:  calc(30vmin / 7.5)!important;
  margin-right:  calc(30vmin / 7.5)!important;
}
.sweet-image .image-content .btn-cancel{
  display: inline-block;
  background: #ccc;
  color: #fff;
  vertical-align: middle;
  width: calc(50% - 60vmin / 7.5)!important;
  margin-left:  calc(30vmin / 7.5)!important;
  margin-right:  calc(30vmin / 7.5)!important;
}
.sweet-image .image-content .image-select-content{
  height: calc(100% - 114vmin / 7.5);
  width: 100%;
}
.sweet-image .image-content .btn-content{
  height: calc(114vmin / 7.5);
  width: 100%;
  text-align: center;
  overflow: hidden;
}
.sweet-image .image-show{
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
}
.sweet-image .image-select{
  background-repeat: no-repeat;
  z-index: 2;
}
.image-select .image-c{
  width: calc(40% + 2px);
  height: calc(40% + 2px);
  left: calc(30% - 1px);
  top: calc(30% - 1px); 
  border: 1px solid #ccc;
}
.image-select .image-t-l{
  width: 30%;
  height: 30%;
  left: 0;
  top: 0;
  border: 1px solid #ccc;
  border-top: none;
  border-left: none;
}
.image-select .image-t-l::before{
  content: " ";
  position: absolute;
  width: 4px;
  height: 100%;
  left: 0;
  top: 0;
  border-right: 1px solid #ccc;
}
.image-select .image-t-l::after{
  content: " ";
  position: absolute;
  height: 4px;
  width: 100%;
  left: 0;
  top: 0;
  border-bottom: 1px solid #ccc;
}
.image-select .image-t-r{
  width: 30%;
  height: 30%;
  top: 0;
  right: 0;
  border: 1px solid #ccc;
  border-top: none;
  border-right: none;
}
.image-select .image-t-r::before{
  content: " ";
  position: absolute;
  width: 4px;
  height: 100%;
  top: 0;
  right: 0;
  border-left: 1px solid #ccc;
}
.image-select .image-t-r::after{
  content: " ";
  position: absolute;
  height: 4px;
  width: 100%;
  top: 0;
  right: 0;
  border-bottom: 1px solid #ccc;
}
.image-select .image-b-l{
  width: 30%;
  height: 30%;
  left: 0;
  bottom: 0;
  border: 1px solid #ccc;
  border-bottom: none;
  border-left: none;
}
.image-select .image-b-l::before{
  content: " ";
  position: absolute;
  width: 4px;
  height: 100%;
  left: 0;
  bottom: 0;
  border-right: 1px solid #ccc;
}
.image-select .image-b-l::after{
  content: " ";
  position: absolute;
  height: 4px;
  width: 100%;
  left: 0;
  bottom: 0;
  border-top: 1px solid #ccc;
}
.image-select .image-b-r{
  width: 30%;
  height: 30%;
  right: 0;
  bottom: 0;
  border: 1px solid #ccc;
  border-bottom: none;
  border-right: none;
}
.image-select .image-b-r::before{
  content: " ";
  position: absolute;
  width: 4px;
  height: 100%;
  right: 0;
  bottom: 0;
  border-left: 1px solid #ccc;
}
.image-select .image-b-r::after{
  content: " ";
  position: absolute;
  height: 4px;
  width: 100%;
  right: 0;
  bottom: 0;
  border-top: 1px solid #ccc;
}
.sweet-image .image-canvas{
  position: fixed;
  top: calc(9999vmin / 7.5);
  left: 0;
}